/* pages/shouye.wxss */
page{
   background-color: #f4f6f7;
}
swiper{
   width: 100%;
   height: 360rpx;
}
.haibao{
   width: 100%;
   height: 360rpx;
}
.haibao image{width: 100%;height:360rpx;}
.find-main{
   width: 90%;
   height: 400rpx;
   margin: auto;
   /* border: 1rpx #dfe6e9 solid; */
   display: flex;
   flex-direction: column;
   justify-content: center;
   align-items: center;
   border-radius: 10rpx;
   box-shadow: 0 0.03rem 0.09rem rgba(0,0,0,0.12), 0 0.16rem 0.36rem rgba(0,0,0,0.12);
   position: relative;
   top: -50rpx;
   background-color: #ffffff;
}
.top{
   width: 100%;
   height: 20%;
   display: flex;
   align-items: center;
   position: relative;
}
.top-left{
   height: auto;
   width: 180rpx;
   display: flex;
   justify-content: space-between;
   align-items: center;
   position: absolute;
   left: 50rpx;
}
.top-left>image{
   height: 25rpx;
   width: 25rpx;
}
.title{
   color: #95a5a6;
   font-size: 25rpx;
}
.title-msg{
   font-size: 25rpx;
   font-weight: 500;
}
.top-right{
   height: 100%;
   width: 200rpx;
   display: flex;
   justify-content: space-between;
   align-items: center;
   position: absolute;
   right: 50rpx;
}
.mid{
   width: 100%;
   height: 60%;
}
.mid-score{
   width: 80%;
   height: 80rpx;
   margin: auto;
   display: flex;
   flex-wrap: nowrap;
   border-radius: 5rpx;
}
.mid-score>input{
   width: 90%;
   height: 90%;
   margin: auto;
   text-align: center;
   font-size:30rpx;
   background-color: #FFF;
}
.icon{
   position: absolute;
   top: 190rpx;
   left: 530rpx;
   font-size: 35rpx;
   color: #95a5a6;
   z-index: 3;
}
.placeholder{
   margin-left: -30rpx;
   margin-top: -2rpx;
   text-align: center;
   background-color: #FFF;
   font-size:30rpx;
}
.mid-title{
   width: 100%;
   height: auto;
   text-align: center;
   margin-bottom: 45rpx;
   font-size: 35rpx;
}

.end{
   width: 100%;
   height: 20%;
   position: relative;
   top: 43rpx;
   display: flex;
   justify-content: center;
   align-items: center;
}
.button{
   height: 100%;
   width: 80%;
   background-color: #686de0;
   border-radius: 50rpx;
   font-size: 30rpx;
   color: #fff;
   display: flex;
   align-items: center;
   justify-content: center;
   transition: background-color 0.5s;
}
.b_click{
   background-color: #673AB7;
}