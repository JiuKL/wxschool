<view class="container">
  <view class="weui-flex l-msg" wx:if="{{dclass.length==0}}">
    <view class="weui-flex__item">
      <view class="placeholder">暂无</view>
    </view>
  </view>
  <view class="container" wx:for="{{dclass}}" wx:key="index">
    <view class="weui-flex l-msg">
      <view class="weui-flex__item">
        <view class="placeholder">{{dclass[index]}}</view>
      </view>
    </view>
  </view>
</view>